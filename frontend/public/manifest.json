{
  "name": "Doctor Who Library",
  "short_name": "DW Library",
  "description": "A comprehensive media library for Doctor Who content with TARDIS Wiki integration",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#003B6D",
  "theme_color": "#003B6D",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "categories": ["entertainment", "education", "reference"],
  "screenshots": [
    {
      "src": "/images/screenshot-narrow.png",
      "sizes": "640x1136",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/images/screenshot-wide.png",
      "sizes": "1024x768",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "icons": [
    {
